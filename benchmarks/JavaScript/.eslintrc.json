{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "max-classes-per-file": "off",
        "class-methods-use-this": "off",
        "no-restricted-syntax": ["error", "WithStatement"],
        "comma-dangle": "off",
        "no-param-reassign": "off",
        "no-nested-ternary": "off",
        "yoda": "off"
    }
}
